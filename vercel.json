{
  "rewrites": [
    {
      "source": "/simulador",
      "destination": "/simulador-ecossistema.html"
    },
    {
      "source": "/debug",
      "destination": "/debug.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/*.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
